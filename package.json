{
  "name": "pi-acp",
  "version": "0.0.21",
  "description": "ACP adapter for pi coding agent",
  "license": "MIT",
  "author": "Sergii Kozak <svkozak@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/svkozak/pi-acp.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "bugs": {
    "url": "https://github.com/svkozak/pi-acp/issues"
  },
  "homepage": "https://github.com/svkozak/pi-acp#readme",
  "keywords": [
    "acp",
    "agent-client-protocol",
    "pi-coding-agent",
    "adapter"
  ],
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pi-acp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "node --import tsx --test test/**/*.test.ts",
    "smoke": "node scripts/smoke-acp.mjs",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "@agentclientprotocol/sdk": "^0.12.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.18.0"
  }
}
